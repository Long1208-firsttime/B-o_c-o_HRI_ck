<launch>
<env name="GAZEBO_MODEL_PATH" value="$(find robot10):$(optenv GAZEBO_MODEL_PATH)"/>
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find robot10)/world/restaurant.world"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
  </include>
<node name="spawn_kid" pkg="gazebo_ros" type="spawn_model" 
        args="-file $(find robot10)/human_kid/model.sdf -sdf -model human_kid -x 2 -y 0 -z 0" 
        output="screen" />
  <param name="robot_description" textfile="$(find robot10)/urdf/robot10.urdf" />
  
  <node name="spawn_model" pkg="gazebo_ros" type="spawn_model"
    args="-param robot_description -urdf -model robot10" output="screen"/>

  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />
  <node name="joint_state_publisher_gui" pkg="joint_state_publisher_gui" type="joint_state_publisher_gui" />
  

  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find robot10)/urdf.rviz" />

</launch>
